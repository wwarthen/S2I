0102   0000             ; 0x8C	CURRENT SCSI BUS STATUS		SELECT ENABLE REGISTER
0103   0000             ; 0x8D  BUS & STATUS REGISTER		START DMA SEND
0104   0000             ; 0x8E	INPUT DATA REGISTER		START DMA TARGET RECEIVE
0105   0000             ; 0x8F	RESET PARITY/INTERRUPT		START DMA INITIATOR RECEIVE
0106   0000             ;
0107   0000             ; -----------------------------------------------
0108   0000             ; PARALLEL PORT (IDE) INTERFACE (I/O BASE + 0x00)
0109   0000             ; -----------------------------------------------
0110   0000             ; PORT	PPI				IDE
0111   0000             ; ----	------------------------------ 	------------------------------
0112   0000             ; 0x80  PORT A				LSB
0113   0000             ; 0x81  PORT B				MSB
0114   0000             ; 0x82  PORT C				IDE CONTROL
0115   0000             ; 0x83  CONTROL
0116   0000             ;
0117   0000             ; -----------------------------------------------
0118   0000             ; CONFIG JUMPER P1 (I/O BASE + 0x04)
0119   0000             ; -----------------------------------------------
0120   0000             ; BIT(S)	DEFINITION
0121   0000             ; ------	------------------------------------------------------
0122   0000             ; 0-2		SCSI BUS DEVICE ID
